<Window
  x:Class="H.ReactiveUI.Apps.Views.MainView"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:views="using:H.ReactiveUI.Apps.Views"
  Icon="/Assets/avalonia-logo.ico"
  Title="H.ReactiveUI.Apps.Avalonia"
  mc:Ignorable="d"
  d:DesignWidth="800"
  d:DesignHeight="450"
  >
  <TabControl>
    <TabItem Header="File">
      <views:FileInteractionsView/>
    </TabItem>
    <TabItem Header="Message">
      <views:MessageInteractionsView/>
    </TabItem>
    <TabItem Header="Web">
      <views:WebInteractionsView/>
    </TabItem>
  </TabControl>
</Window>
